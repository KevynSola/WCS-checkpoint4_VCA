<div class="form-target-killer">
	{{ form_start(form) }}
	<div class="bloc-already d-none">
		{{ form_row(form.lastname) }}
		{{ form_row(form.firstname) }}
		{{ form_row(form.city) }}
	</div>
	<div class="form-group">
		{{ form_row(form.posterFile) }}
		{{ form_row(form.isKilled) }}
		<button class="btn btn-lg btn-outline-light my-5">{{ button_label|default('Save') }}</button>
	</div>
	{{ form_end(form) }}
</div>
